{
  "name": "vite-hono",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run build.client && pnpm run build.server",
    "build.client": "vite build --mode client && vite build",
    "build.prerender": "tsx src/prerender.ts",
    "build.server": "vite build --mode production --ssr",
    "depcheck": "depcheck --ignores=\"@trivago/prettier-plugin-*,prettier-plugin-*,babel-plugin-*\"",
    "dev": "vite dev --port 9999",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "pnpm build && pnpm start",
    "serve": "pnpx serve dist/public",
    "start": "node dist/server.js --port 9999",
    "typecheck": "tsc --noemit"
  },
  "dependencies": {
    "@hono/vite-ssg": "^0.1.1",
    "@tanstack/react-router": "^1.98.4",
    "@tanstack/start": "^1.98.5",
    "hono": "^4.6.19",
    "hono-pino": "^0.7.0",
    "node": "link:@hono/vite-build/node",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stoker": "^1.4.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@hono/node-server": "^1.13.7",
    "@hono/vite-build": "^1.3.0",
    "@hono/vite-dev-server": "^0.18.1",
    "@hono/zod-validator": "^0.4.2",
    "@t3-oss/env-core": "^0.11.1",
    "@tailwindcss/vite": "^4.0.1",
    "@tanstack/router-devtools": "^1.98.4",
    "@tanstack/router-plugin": "^1.98.5",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "babel-plugin-react-compiler": "19.0.0-beta-decd7b8-20250118",
    "dotenv": "^16.4.7",
    "eslint": "^9.19.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-decd7b8-20250118",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "8.4.47",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.1",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "hono": "patches/hono.patch"
    }
  }
}
