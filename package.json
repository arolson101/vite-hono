{
  "name": "vite-hono",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm typecheck && pnpm lint && pnpm build:client && pnpm build:server",
    "build:client": "vite build --mode client",
    "build:server": "tsup-node",
    "depcheck": "depcheck --ignores=\"@trivago/prettier-plugin-*,prettier-plugin-*,babel-plugin-*\"",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "pnpm build && pnpm start",
    "start": "cd dist && node --env-file=../.env serve-node.js",
    "typecheck": "tsc --noemit"
  },
  "dependencies": {
    "hono": "^4.6.18",
    "hono-pino": "^0.7.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stoker": "^1.4.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@hono/node-server": "^1.13.7",
    "@hono/vite-dev-server": "^0.18.1",
    "@hono/zod-validator": "^0.4.2",
    "@t3-oss/env-core": "^0.11.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-react-compiler": "19.0.0-beta-decd7b8-20250118",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-plugin-react-compiler": "19.0.0-beta-decd7b8-20250118",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsup": "^8.3.6",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.7",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "patchedDependencies": {
      "hono@4.6.18": "patches/hono@4.6.18.patch"
    }
  }
}
